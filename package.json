    "fluxxor": "^1.5.2",
    "jquery": "~2.1.3",
    "jquery.cookie": "~1.4.1",
    "lodash": "~3.6.0",
    "node-static": "~0.7.6",
    "pnglib": "0.0.1",
    "react": "^0.13.1",
    "react-bootstrap": "^0.20.2",
    "react-router": "^0.13.2",
    "reactify": "^1.1.0",
    "web3": "../web3.js"
  },
  "devDependencies": {
