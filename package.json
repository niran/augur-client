  },
  "engines": {
    "node": "0.12.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AugurProject/augur-client.git"
  },
  "bugs": {
    "url": "https://github.com/AugurProject/augur-client/issues"
  },
